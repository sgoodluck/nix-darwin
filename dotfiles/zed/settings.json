{
  "theme": {
    "mode": "dark",
    "dark": "One Dark",
    "light": "One Light"
  },
  "ui_font_size": 16,
  "buffer_font_size": 12,
  "buffer_font_family": "CaskaydiaCove Nerd Font",
  "terminal": {
    "font_family": "CaskaydiaCove Nerd Font",
    "font_size": 12
  },
  "experimental.theme_overrides": {
    "background": "#1d2235",
    "editor.background": "#1d2235",
    "editor.foreground": "#ffffff",
    "editor.line_number": "#989898",
    "editor.active_line_number": "#ffffff",
    "terminal.background": "#1d2235",
    "terminal.foreground": "#ffffff",
    "terminal.ansi.black": "#1d2235",
    "terminal.ansi.red": "#ff5f59",
    "terminal.ansi.green": "#44bc44",
    "terminal.ansi.yellow": "#d0bc00",
    "terminal.ansi.blue": "#2fafff",
    "terminal.ansi.magenta": "#feacd0",
    "terminal.ansi.cyan": "#00d3d0",
    "terminal.ansi.white": "#989898",
    "terminal.ansi.bright_black": "#4f5666",
    "terminal.ansi.bright_red": "#ff6b55",
    "terminal.ansi.bright_green": "#70c900",
    "terminal.ansi.bright_yellow": "#fec43f",
    "terminal.ansi.bright_blue": "#79a8ff",
    "terminal.ansi.bright_magenta": "#f78fe7",
    "terminal.ansi.bright_cyan": "#4ae2f0",
    "terminal.ansi.bright_white": "#ffffff",
    "editor.selection.background": "#5a5a5a",
    "text": "#ffffff",
    "text.muted": "#a8a8a8",
    "title_bar.background": "#1d2235",
    "toolbar.background": "#1d2235",
    "tab_bar.background": "#1d2235",
    "tab.inactive_background": "#1d2235",
    "tab.active_background": "#2a2f4a",
    "panel.background": "#1d2235",
    "status_bar.background": "#1d2235"
  },
  "vim_mode": true,
  "autosave": "on_focus_change",
  "format_on_save": "on",
  "tab_size": 2,
  "soft_wrap": "editor_width"
}
